{
    "version": 2,
    "builds": [
        {
            "src": "Backend/Jobex/app/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "Backend/Jobex/app/main.py"
        }
    ],
    "env": {
        "PYTHONPATH": "Backend/Jobex"
    }
} 