{
  "version": 2,
  "builds": [
      {
          "src": "Backend/Jobex/api/main.py",
          "use": "@vercel/python"
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "Backend/Jobex/api/main.py"
      }
  ],
  "functions": {
      "Jobex/api/main.py": {
          "memory": 1024,
          "maxDuration": 60
      }
  }
}