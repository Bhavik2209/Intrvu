{
    "version": 2,
    "builds": [
      {
        "src": "Backend/Jobex/api/index.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "Backend/Jobex/api/index.py"
      }
    ],
    "env": {
      "PYTHONPATH": "Backend/Jobex"
    }
  }