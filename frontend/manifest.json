{
    "name": "IntrvuFit - Resume optimizer",
    "version": "0.0.5",
    "manifest_version": 3,
    "action": {
        "default_width": 800,
        "default_height": 600
    },
    "side_panel": {
        "default_path": "popup.html"
    },
    "permissions": [
        "activeTab",
        "scripting",
        "sidePanel"
    ],
    "host_permissions": [
        "https://www.linkedin.com/jobs/*",
        "http://localhost:8000/*"
    ]
}